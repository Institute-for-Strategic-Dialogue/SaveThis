{
  "manifest_version": 3,
  "name": "Save This - Analyst Asstiant",
  "version": "0.1.0",
  "description": "Save terms, profiles, and URLs into Airtable with tags, projects, and more.",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Save to OSINT base"
  },
  "options_page": "options.html"
}